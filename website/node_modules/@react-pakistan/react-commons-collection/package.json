{
  "name": "@react-pakistan/react-commons-collection",
  "version": "6.0.4",
  "description": "React Commons Collection built upon React StoryBook",
  "module": "lib/index.js",
  "main": "lib/index.js",
  "types": "index.d.tsx",
  "scripts": {
    "build": "yarn clean && yarn build:ts && cp package.json lib && cp README.md lib",
    "build:storybook": "build-storybook -c .storybook -o .out",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:ts:cjs": "tsc --project tsconfig.build-cjs.json",
    "clean": "yarn rimraf ./lib",
    "copy:deploy:files": "cp stories.json ./*out*/ && cp sitemap.xml ./*out*/ && cp favicon.ico ./*out*/ && cp assets/react-pakistan.jpeg ./*out*/",
    "deploy:chromatic": "yarn chromatic --project-token=wb80o89004",
    "deploy:storybook": "concurrently \"yarn storybook:ghpages\" \"sleep 15 && rm ./*out*/favicon.ico && yarn copy:deploy:files\"",
    "extract": "yarn build:storybook && sb extract ./.out && cp ./.out/stories.json . && rm -rf ./.out",
    "generate:sitemap": "scripts/generate-sitemap.sh",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:css": "stylelint './src/**/*.tsx'",
    "lint:staged": "lint-staged",
    "lint:ts": "eslint --config eslint.config.js",
    "publish:github": "yarn build && cd lib/ && npm publish && cd ..",
    "publish:npm": "yarn build && cd lib/ && npm publish --access public && cd ..",
    "screenshot": "npx storycap http://localhost:9001",
    "start": "start-storybook -p 9001 -c .storybook",
    "storybook:ghpages": "storybook-to-ghpages",
    "test": "jest --coverage --config jest/config.json",
    "test:badges": "yarn test && jest-coverage-badges",
    "upgrade": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Taimoormk/react-commons-collection.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:staged",
      "pre-push": "yarn test && yarn build"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "keywords": [
    "a",
    "audio",
    "background image",
    "button",
    "canvas",
    "check box",
    "child grid",
    "color input",
    "colors",
    "column container",
    "date input",
    "date time input",
    "file input",
    "footer",
    "grid",
    "h1",
    "h2",
    "h3",
    "h4",
    "h5",
    "header",
    "hoc",
    "hr",
    "iframe",
    "image",
    "intro",
    "li",
    "month input",
    "progress bar",
    "radio button",
    "range input",
    "row container",
    "select",
    "social share",
    "spacer",
    "table",
    "text",
    "text area",
    "text input",
    "time input",
    "typography",
    "ul",
    "video",
    "week input",
    "wrapper"
  ],
  "author": "Taimoor Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Taimoormk/react-commons-collection/issues"
  },
  "homepage": "https://github.com/Taimoormk/react-commons-collection#readme",
  "dependencies": {
    "react-share": "^4.3.1",
    "styled-components": "^5.2.0"
